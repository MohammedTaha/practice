<!DOCTYPE html>
<html>
  <head>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>

<script>
$(function(){
$("#btn-ajax").on('click', function(){
    alert("Sending Ajax request");
    $.ajax({
        type: 'get',
        url : '/helloWorld',
        success: function(data){
            alert("Succeed");
            console.log(data);
            $("#testDiv").html(data.a + data.b);
        },
        error: function(){
            alert("Error");
        }
    });
});

});
</script>
  </head>
  <body>

    Hello world
    <button id="btn-ajax">Click Me </button>
    <div id="testDiv">Container</div>

  </body>
</html>